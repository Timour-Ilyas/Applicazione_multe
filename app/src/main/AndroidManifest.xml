<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caramba">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_caramba"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_caramba_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Caramba">
        <activity
            android:name=".ActivityInfo"
            android:exported="false" />
        <activity
            android:name=".Impostazioni"
            android:exported="false" />
        <activity
            android:name=".RecuperoCredenziali"
            android:exported="false" />
        <activity
            android:name=".ListaMulte"
            android:exported="false" />
        <activity
            android:name=".FaiMulta"
            android:exported="false" />
        <activity
            android:name=".MenuPrincipale"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>